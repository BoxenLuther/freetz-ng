[ "FREETZ_PACKAGE_IMAGEMAGICK_VERSION_ABANDON" == "y" ] && ELIBVER="10.0.0" || ELIBVER="10.0.2"
if [ "$EXTERNAL_FREETZ_PACKAGE_IMAGEMAGICK" ]; then
	[ "$FREETZ_PACKAGE_IMAGEMAGICK"        == "y" ] && EXTERNAL_FILES+=" /usr/bin/magick"
	[ "$FREETZ_PACKAGE_IMAGEMAGICK_STATIC" != "y" ] && EXTERNAL_FILES+=" ${FREETZ_LIBRARY_DIR}/libMagickCore-7-Q16.so.$ELIBVER"
	[ "$FREETZ_PACKAGE_IMAGEMAGICK_STATIC" != "y" ] && EXTERNAL_FILES+=" ${FREETZ_LIBRARY_DIR}/libMagickWand-7-Q16.so.$ELIBVER"
	[ "$FREETZ_PACKAGE_IMAGEMAGICK_xml"    == "y" ] && EXTERNAL_FILES+=" /etc/ImageMagick-7"
fi
